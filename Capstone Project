    }
   ],
   "source": [
    "df_t = df.groupby('benefits')\n",
    "ag = df_t.treatment.value_counts().sort_index()\n",
    "ag.plot(kind='barh', subplots=True, figsize=(12,6), color='indianred')\n",
    "plt.tick_params('both', left=False) \n",
    "plt.xlabel(\"Number of people\")\n",
    "plt.ylabel(\"Has benefits?, Has sought treatment?\")\n",
